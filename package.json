{
  "description": "input for numeric values",
  "keywords": [
    "number",
    "integer",
    "input",
    "polymer",
    "webcomponents",
    "unit",
    "min",
    "max"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fooloomanzoo/number-input.git"
  },
  "homepage": "https://github.com/fooloomanzoo/number-input/",
  "name": "@fooloomanzoo/number-input",
  "version": "3.0.3",
  "main": "index.js",
  "authors": [
    "Johannes Brautzsch <fooloomanzoo>"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublishOnly": "npm run build && git add . && git commit -m \"publish new version\" && npm version patch && git push",
    "gh-pages": "git checkout gh-pages && git pull && npm update -f && git add . && git commit -m \"update\" && git push && git checkout master",
    "build": "git pull && npm update && npm run analyze",
    "start": "npm run analyze && polymer serve -o",
    "analyze": "polymer analyze > analysis.json",
    "test": "polymer test",
    "test:p": "polymer test -p --config-file=wct.p.conf.js"
  },
  "license": "MIT",
  "dependencies": {
    "@fooloomanzoo/input-picker-pattern": "^3.0.8",
    "@fooloomanzoo/property-mixins": "^3.0.7",
    "@polymer/polymer": "^3"
  },
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2",
    "@polymer/iron-component-page": "^4",
    "@polymer/iron-flex-layout": "^3",
    "@polymer/iron-demo-helpers": "^3",
    "@polymer/iron-form": "^3",
    "@polymer/iron-test-helpers": "^3",
    "wct-browser-legacy": "^1"
  },
  "author": "Johannes Brautzsch <fooloomanzoo>"
}
